{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "watch:tailwind": "postcss public/tailwind.css -o public/index.css -w",
    "build:tailwind": "NODE_ENV=production postcss public/tailwind.css -o public/index.css",
    "dev": "run-p start:dev autobuild watch:tailwind",
    "build": "yarn build:tailwind && rollup -c",
    "autobuild": "rollup -c -w",
    "start": "sirv public --single",
    "start:dev": "sirv public --dev --single",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "format": "yarn prettier --write src/*"
  },
  "devDependencies": {
    "@modular-css/svelte": "^25.7.0",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.3.1",
    "aos": "^2.3.4",
    "axios": "^0.19.2",
    "js-cookie": "^2.2.1",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "prettier": "2.0.5",
    "prettier-plugin-svelte": "^0.7.0",
    "rollup": "^1.20.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.0.0",
    "svelte-feather-icons": "^3.2.2",
    "svelte-preprocess": "^3.6.0",
    "svelte-routing": "^1.4.2",
    "tailwindcss-plugins": "^0.3.0"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@storybook/cli": "^5.3.18",
    "@sveltejs/svelte-scroller": "^2.0.6",
    "init": "^0.1.2",
    "postcss": "^7.0.27",
    "postcss-js": "^2.0.3",
    "postcss-purgecss": "^2.0.3",
    "query-string": "^6.12.1",
    "rellax": "^1.12.1",
    "rollup-plugin-tailwind": "^1.0.0",
    "sapper-environment": "^1.0.1",
    "sb": "^5.3.18",
    "sirv-cli": "^0.4.4",
    "tailwind": "^4.0.0",
    "tailwindcss": "^1.2.0"
  }
}
