{
    "name": "kg-portfolio-svelte-app",
    "version": "1.0.0",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/K-JAX/kevingarubba.com"
    },
    "scripts": {
        "preinstall": "npx npm-force-resolutions",
        "watch:tailwind": "postcss public/tailwind.css -o public/index.css -w",
        "build:tailwind": "postcss public/tailwind.css -o public/index.css",
        "dev": "export NODE_ENV=development && run-p start:dev autobuild watch:tailwind",
        "build": "export NODE_ENV=production && npm run build:tailwind && rollup -c",
        "autobuild": "rollup -c -w",
        "start": "sirv public --single",
        "start:dev": "sirv public --dev --single --port 8081 --host",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "format": "npm run prettier --write src/*"
    },
    "devDependencies": {
        "@babel/core": "^7.16.0",
        "@babel/preset-env": "^7.16.4",
        "@modular-css/svelte": "^25.7.0",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^11.0.0",
        "@rollup/plugin-node-resolve": "^7.0.0",
        "@rollup/plugin-replace": "^2.3.1",
        "aos": "^2.3.4",
        "axios": "^0.24.0",
        "js-cookie": "^2.2.1",
        "json-schema": "^0.4.0",
        "lodash": "^4.17.21",
        "node-sass": "^6.0.1",
        "npm-run-all": "^4.1.5",
        "postcss-cli": "^7.1.0",
        "prettier": "^1.0.0",
        "prettier-plugin-svelte": "^0.7.0",
        "rollup": "^1.20.0",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-inject-process-env": "^1.3.1",
        "rollup-plugin-livereload": "^1.0.0",
        "rollup-plugin-svelte": "^6.1.1",
        "rollup-plugin-terser": "^5.1.2",
        "sass": "^1.43.4",
        "svelte": "^3.49.0",
        "svelte-feather-icons": "^3.2.2",
        "svelte-preprocess": "^3.6.0",
        "svelte-routing": "^1.4.2",
        "tailwindcss-plugins": "^0.3.0",
        "trim": "^1.0.1"
    },
    "dependencies": {
        "@fullhuman/postcss-purgecss": "^2.1.0",
        "@storybook/cli": "^5.3.18",
        "@sveltejs/svelte-scroller": "^2.0.6",
        "init": "^0.1.2",
        "postcss": "^7.0.27",
        "postcss-js": "^2.0.3",
        "postcss-purgecss": "^2.0.3",
        "query-string": "^6.12.1",
        "rellax": "^1.12.1",
        "rollup-plugin-tailwind": "^1.0.0",
        "sapper-environment": "^1.0.1",
        "sb": "^5.3.18",
        "sirv-cli": "^0.4.4",
        "svelte-recaptcha-v2": "^0.0.2",
        "tailwind": "^4.0.0",
        "tailwindcss": "^1.2.0"
    },
    "resolutions": {
        "json-schema": "^0.4.0",
        "lodash": "^4.17.21",
        "trim": "^1.0.1"
    }
